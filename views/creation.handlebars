<section class='general'>
    <div class='container'>
        <div class='general--creation'>
            <div class='general--photo'>
                <img src='/img/placeholder.png' alt='Your general here!' class='general--photo--set'>
            </div>
            <div class='general--data'>
                <div class='general--data--wrapper'>
                    <div class='data--entry firstName'>
                        <label for='firstName' class='general-data-label'>General Name:</label>
                        <input type='text' class='general--data--collection' id='firstName' name='firstName'>
                    </div>
                    <div class='data--entry photoName'>
                        <label for='photo-collector' class='general-data-label'>Photo Link:</label>
                        <input type='text' class='general--data--collection' name='photo-collector' id='photo-src'>
                    </div>
                    <div class='data--entry pointAllocator'>
                        <p class='general--distribute'>
                            Distribute 10 points!
                        </p>
                        <div class='point--spender'>
                            <label for='attack-collector'>Attack:</label>
                            <input type='number' class='general--data--collection' name='attack--collector' id='attack--collector'
                                min='0' max='10' value='0'>
                            <label for='defense-collector'>Defense:</label>
                            <input type='number' class='general--data--collection' name='defense--collector' id='defense--collector'
                                min='0' max='10' value='0'>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
